{
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 50 6 * * *"
    },

    {
      "type": "twilioSms",
      "name": "message",
      "accountSidSetting": "TWILIO_SID",
      "authTokenSetting": "TWILIO_TOKEN",
      "from": "SENDER_NUMBER",
      "to": "RECIPIENT_NUMBER",
      "direction": "out"
    }
  ]
}
